{
	"name": "client",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "start http://localhost:3000 && next dev",
		"build": "next build",
		"start": "next start",
		"lint": "eslint .",
		"type-check": "tsc --noEmit --pretty",
		"type-check:watch": "tsc --noEmit --pretty --watch",
		"type-check:ci": "tsc --noEmit --pretty --skipLibCheck",
		"lint:all": "eslint . --format=compact --cache --cache-location .eslintcache",
		"lint:fix": "eslint . --fix --cache --cache-location .eslintcache",
		"lint:strict": "eslint . --max-warnings=0 --cache --cache-location .eslintcache",
		"lint:ci": "eslint . --max-warnings=0",
		"check:all": "npm run type-check && npm run lint:all",
		"check:ci": "npm run type-check:ci && npm run lint:ci",
		"check:watch": "npm run type-check:watch",
		"test": "vitest",
		"test:run": "vitest run",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui",
		"test:rbac": "vitest run rbac Permission Role guards",
		"test:cart": "vitest run cart",
		"test:validation": "vitest run validation",
		"test:quotes": "vitest run quotes Quote",
		"test:security": "vitest run rbac Permission guards",
		"test:core": "vitest run _core",
		"test:features": "vitest run _features",
		"test:shared": "vitest run _shared",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:headed": "playwright test --headed",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:trace": "playwright test --trace on",
		"test:e2e:customer": "playwright test --project=customer",
		"test:e2e:fulfillment": "playwright test --project=fulfillment",
		"test:e2e:sales": "playwright test --project=sales-rep",
		"test:e2e:admin": "playwright test --project=admin",
		"test:e2e:super-admin": "playwright test --project=super-admin",
		"test:e2e:public": "playwright test --project=unauthenticated",
		"test:e2e:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
		"test:e2e:cross-browser": "playwright test --project=customer-firefox --project=customer-webkit",
		"test:e2e:customer-journey": "playwright test e2e/journeys/customer/order-lifecycle.spec.ts --project=customer",
		"test:e2e:login": "playwright test e2e/journeys/public/login.spec.ts --project=unauthenticated",
		"test:e2e:report": "playwright show-report",
		"test:e2e:codegen": "playwright codegen",
		"test:e2e:codegen:customer": "playwright codegen --load-storage=.auth/customer.json",
		"test:e2e:codegen:admin": "playwright codegen --load-storage=.auth/admin.json",
		"test:e2e:install": "playwright install --with-deps",
		"test:all": "npm run test:run && npm run test:e2e",
		"test:ci": "vitest run --reporter=json --reporter=verbose --outputFile=test-results.json",
		"test:ci:coverage": "vitest run --coverage --reporter=json --outputFile=test-results.json",
		"test:ci:e2e": "playwright test --reporter=github --reporter=html"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^8.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@hookform/resolvers": "^3.9.1",
		"@tanstack/react-query": "^5.90.16",
		"@tanstack/react-table": "^8.20.5",
		"@tanstack/react-virtual": "^3.13.12",
		"@types/js-cookie": "^3.0.6",
		"axios": "^1.6.7",
		"classnames": "^2.5.1",
		"cookies-next": "^4.1.1",
		"date-fns": "^3.6.0",
		"embla-carousel-react": "^8.6.0",
		"exceljs": "^4.4.0",
		"framer-motion": "^12.23.24",
		"js-cookie": "^3.0.5",
		"jspdf": "^4.0.0",
		"jspdf-autotable": "^5.0.2",
		"lodash": "^4.17.21",
		"lucide-react": "^0.552.0",
		"next": "^16.1.1",
		"papaparse": "^5.5.3",
		"qrcode.react": "^4.2.0",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"react-hook-form": "^7.53.2",
		"react-toastify": "^10.0.4",
		"sharp": "^0.33.4",
		"zod": "^3.23.8",
		"zustand": "^5.0.8"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.3",
		"@next/eslint-plugin-next": "^16.1.1",
		"@playwright/test": "^1.57.0",
		"@tailwindcss/postcss": "^4.1.0",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/lodash": "^4.17.0",
		"@types/node": "^20",
		"@types/papaparse": "^5.5.2",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@typescript-eslint/eslint-plugin": "^8.47.0",
		"@typescript-eslint/parser": "^8.47.0",
		"@vitejs/plugin-react": "^5.1.1",
		"@vitest/coverage-v8": "^4.0.15",
		"@vitest/ui": "^4.0.12",
		"autoprefixer": "^10.4.22",
		"babel-plugin-react-compiler": "^1.0.0",
		"daisyui": "^5.3.7",
		"dotenv": "^17.2.3",
		"eslint": "^9.39.1",
		"eslint-config-next": "^16.1.1",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.5",
		"globals": "^16.5.0",
		"jest-axe": "^9.0.0",
		"jsdom": "^27.2.0",
		"msw": "^2.12.2",
		"postcss": "^8",
		"tailwindcss": "^4.1.0",
		"typescript": "^5.9.3",
		"vite-tsconfig-paths": "^6.0.1",
		"vitest": "^4.0.12"
	}
}
